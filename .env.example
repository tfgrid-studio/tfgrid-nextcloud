# TFGrid Nextcloud Configuration
# Copy this file to .env and customize

# Required: Public domain for Nextcloud
DOMAIN=cloud.example.com

# Optional: Custom data directory (default: Docker volume)
# NEXTCLOUD_DATADIR=/mnt/data/nextcloud

# Optional: Upload limit (default: 10G)
# NEXTCLOUD_UPLOAD_LIMIT=10G

# Optional: PHP memory limit (default: 512M)
# NEXTCLOUD_MEMORY_LIMIT=512M

# Optional: Timezone (default: UTC)
# NEXTCLOUD_TIMEZONE=America/New_York

# Optional: Trusted proxies (comma-separated)
# NEXTCLOUD_TRUSTED_PROXIES=
